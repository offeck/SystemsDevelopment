{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "work1: (gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/work_1/bin/dj_manager",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/work_1",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "work1: build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
